{
  "name": "connect-frontend-demo",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 20.9.0"
  },
  "packageManager": "pnpm@9.11.0",
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.11.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:next": "next dev",
    "build:next": "next build",
    "start:next": "next start",
    "check:ts": "tsc --noEmit --project tsconfig.json",
    "check:lint": "biome lint --write ./",
    "check:format": "biome format --write ./",
    "check:all": "pnpm check:format && pnpm check:lint && pnpm check:ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.3",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "next": "14.2.11",
    "next-intl": "^3.19.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-markdown": "8.0.7",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/typescript": "^4.0.9",
    "@netlify/plugin-nextjs": "^5.6.0",
    "@types/node": "20.16.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "postcss": "^8.4.41",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  }
}
